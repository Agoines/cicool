<view class="page">
    <view class="cover-box" style="text-align: center;">
        <image src="../../src/partying.png" class="emoji" mode="widthFix"></image>
        <text class="title">恭喜完成本次单词{{wordType}}</text>
        <text class="sub_title">一共{{wordType}} {{wordNum}} 个单词</text>
    </view>

    <view class="footer_container">
        <view class="fab" bind:tap="backIndex">
            <text class="fab_text">确定</text>
        </view>
    </view>
</view>
